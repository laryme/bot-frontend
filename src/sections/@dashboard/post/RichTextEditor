import React, { useState } from "react";
import { QuillEditor } from "quill";

const RichTextEditor = () => {
  const [value, setValue] = useState("");

  const handleChange = (value) => {
    setValue(value);
  };

  return (
    <QuillEditor
      value={value}
      onChange={handleChange}
      theme="snow"
    />
  );
};

export default RichTextEditor;